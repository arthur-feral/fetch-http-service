{
  "name": "fetch-http-service",
  "version": "0.1.0",
  "description": "HTTP service using fetch API",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "$(npm bin)/babel . --out-dir dist --ignore tests,node_modules,nyc-reports,dist",
    "prepublish": "yarn run transpile",
    "lint:js": "eslint lib tests --ext .js ",
    "test": "nyc --check-coverage --report-dir=${CIRCLE_ARTIFACTS:-'./nyc-reports'} --reporter=lcov yarn run mocha && yarn run lint:js",
    "mocha": "mocha --compilers js:babel-register -r tests/bootstrap.test.js $(find lib -name '*.test.js')",
    "try": "yarn run transpile && babel-node --presets env test.js"
  },
  "author": "Arthur FERAL <arthur.feral@hotmail.fr>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "sinon": "^5.0.10"
  },
  "dependencies": {
    "lodash.reduce": "^4.6.0",
    "query-string": "^6.1.0",
    "url-parse": "^1.4.0",
    "whatwg-fetch": "^2.0.4"
  },
  "nyc": {
    "exclude": [
      "dist/"
    ]
  }
}
